<w3fu:stylesheet version="1.0"
	xmlns:w3fu="http://www.w3.org/1999/XSL/Transform">

	<w3fu:template name="w3fu:datepicker">
		<div class="datepicker-container">
			<div class="datepicker-control">
				<form><p><span class="datepicker-prev">Пред</span><span class="datepicker-next">След</span>
					<select size="1" class="datepicker-control-month">
						<option>Январь</option>
						<option>Февраль</option>
						<option>Март</option>
						<option>Апрель</option>
						<option>Май</option>
						<option>Июнь</option>
						<option>Июль</option>
						<option>Август</option>
						<option>Сентябрь</option>
						<option>Октябрь</option>
						<option>Ноябрь</option>
						<option>Декабрь</option>
					</select>
					<select size="1" class="datepicker-control-year">
						<option>2010</option>
						<option>2011</option>
						<option>2012</option>
						<option>2013</option>
						<option>2014</option>
						<option>2015</option>
						<option>2016</option>
						<option>2017</option>
						<option>2018</option>
						<option>2019</option>
						<option>2020</option>
					</select>
					
				</p></form>		
			</div>
			
			<div class="datepicker-calendar">
				<table class="datepicker-calendar">
					<tr><th></th><th>Пн</th><th>Вт</th><th>Ср</th><th>Чт</th><th>Пт</th><th>Сб</th><th>Вс</th></tr>
					<tr><th class="week"></th><td></td><td></td><td></td><td></td><td></td><td></td><td></td></tr>
					<tr><th class="week"></th><td></td><td></td><td></td><td></td><td></td><td></td><td></td></tr>
					<tr><th class="week"></th><td></td><td></td><td></td><td></td><td></td><td></td><td></td></tr>
					<tr><th class="week"></th><td></td><td></td><td></td><td></td><td></td><td></td><td></td></tr>
					<tr><th class="week"></th><td></td><td></td><td></td><td></td><td></td><td></td><td></td></tr>
					<tr><th class="week"></th><td></td><td></td><td></td><td></td><td></td><td></td><td></td></tr>
				</table>
			</div>
			
		</div>
 		 
	</w3fu:template>
		
</w3fu:stylesheet>