{
	"include": ["forms/components/edit"],
	"body": {
		"all": {
			"fill": {"file": "edit-password.html"},
			"args" : {
				"edit-password" : {
					"call": "forms/components/edit",
					"args":{
						"type"  :"password",
						"class" : "b-edit b-edit-password__element js-element",
						"name" : "password",
						"no-show-error-msg" : "yes",
						"maxlength": "32"
					}
				},
				"edit-password-display" : {
					"call": "forms/components/edit",
					"args":{
						"type"  :"text",
						"class" : "b-edit b-edit-password__monitor js-monitor",
						"name" : "password",
						"maxlength": "32"
					}
				}
			}
		}
	}
}