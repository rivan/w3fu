{
	"include": {
		"edit-password" : "forms/components/edit",
		"edit-password-display" : "forms/components/edit"
	},
	"body": {
		"all": {
			"fill": {"file": "edit-password.html"},
			"args" : {
				"edit-password" : {
					"call": "edit-password",
					"args":{
						"type"  :"password",
						"class" : "b-edit b-edit-password__element js-element",
						"name" : "password",
						"maxlength": "32"
					}
				},
				"edit-password-display" : {
					"call": "edit-password-display",
					"args":{
						"type"  :"text",
						"class" : "b-edit b-edit-password__monitor js-monitor",
						"name" : "",
						"maxlength": "32"
					}
				}
			}
		}
	}
}