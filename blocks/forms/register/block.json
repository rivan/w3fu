{
	"include": ["forms/components/edit", "forms/components/button"],
	"body": {
		"all": {
			"fill": {"file": "form-register.html"},
			"args" : {
				"edit-login" : {
					"call": "forms/components/edit",
					"args":{
						"type" : "text",
						"class" : "b-edit",
						"name" : "email",
						"maxlength": "254"
					}
				},
				"submit" :{
					"call" : "forms/components/button",
					"args" : {
						"class" : "b-button",
						"value" : "ОК"
					}
				}
			}
		}
	}
}